ense\n\nMIT\n\n[npm-image]: https://img.shields.io/npm/v/snake-case.svg?style=flat\n[npm-url]: https://npmjs.org/package/snake-case\n[downloads-image]: https://img.shields.io/npm/dm/snake-case.svg?style=flat\n[downloads-url]: https://npmjs.org/package/snake-case\n[travis-image]: https://img.shields.io/travis/blakeembrey/snake-case.svg?style=flat\n[travis-url]: https://travis-ci.org/blakeembrey/snake-case\n[coveralls-image]: https://img.shields.io/coveralls/blakeembrey/snake-case.svg?style=flat\n[coveralls-url]: https://coveralls.io/r/blakeembrey/snake-case?branch=master\n","maintainers":[{"name":"blakeembrey","email":"hello@blakeembrey.com"}],"time":{"modified":"2015-01-31T11:02:29.007Z","created":"2014-03-24T23:27:57.545Z","0.0.1":"2014-03-24T23:27:57.545Z","0.0.2":"2014-03-25T02:17:08.850Z","0.1.0":"2014-04-09T09:53:47.340Z","0.1.1":"2014-04-09T09:55:17.849Z","1.0.0":"2014-08-17T18:50:22.995Z","1.0.1":"2014-08-19T13:46:52.993Z","1.1.0":"2015-01-12T08:26:22.782Z","1.1.1":"2015-01-31T11:02:29.007Z"},"homepage":"https://github.com/blakeembrey/snake-case","keywords":["snake","case","underscore"],"repository":{"type":"git","url":"git://github.com/blakeembrey/snake-case.git"},"author":{"name":"Blake Embrey","email":"hello@blakeembrey.com","url":"http://blakeembrey.me"},"bugs":{"url":"https://github.com/blakeembrey/snake-case/issues"},"license":"MIT","readmeFilename":"README.md","users":{"itonyyo":true},"_attachments":{},"_etag":"\"SH6T54YK7Y951Z81O6ZJBVBX\""}